    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": ["<node_internals>/**"],
            "program": "${workspaceFolder}/demos/gen-capture-pcap/dist/index.js",
            "preLaunchTask": "Build gen-capt-script",
            "outFiles": ["${workspaceFolder}/demos/gen-capture-pcap/dist/**/*.js"],
            "console": "integratedTerminal",
            "resolveSourceMapLocations": ["${workspaceFolder}/**", "!**/node_modules/**"],
            "args": ["gen-capture-pcap", "-b", "192.168.88.252", "-u", "admin", "-p", "admin"]
        }
    ]
}
